<eula_templates.xml>
  <br>
    <class>_TagTemplate</class>
    <args>
      <arg>&lt;br/&gt;</arg>
    </args>
  </br>
  <link>
    <class>_LinkTemplate</class>
    <args>
      <arg>&lt;A HREF="event:%s"&gt;&lt;U&gt;%s&lt;/U&gt;&lt;/A&gt;</arg>
